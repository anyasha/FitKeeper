# platform :ios, '9.0'

target 'FitKeeper' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'FSCalendar', '~> 2.7'
  pod ‘Firebase’
  pod ‘Firebase/Core’
  pod ‘Firebase/Auth’
  pod ‘Firebase/Database’
  pod ‘Firebase/Storage’
  pod 'PieCharts'
  pod 'SwiftChart'

end
